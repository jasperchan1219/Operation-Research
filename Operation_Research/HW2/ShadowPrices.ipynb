    "import numpy as np\n",
    "A = np.matrix([\n",
    "    [6, 5],    \n",
    "    [10, 20],\n",
    "])\n",
    "b = np.matrix([\n",
    "    [62],\n",
    "    [150]\n",
    "])\n",
    "X = np.linalg.solve(A,b)\n",
    "x1 = X[0,0]\n",
    "x2 = X[1,0]\n",
    "z = 500*x1 + 450*x2\n",
    "print(\"z: \"+str(z))\n",
    "print(\"ShadowPrice is: \"+str(z-(5221*7+3)/7))\n"
   ]
  }
